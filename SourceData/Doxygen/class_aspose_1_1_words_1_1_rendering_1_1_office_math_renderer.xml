<codeline><highlight class="normal">ASSERT_EQ(159,<sp/>bounds.get_Width());</highlight></codeline>
<codeline><highlight class="normal">ASSERT_EQ(18,<sp/>bounds.get_Height());</highlight></codeline>
<codeline><highlight class="normal"></highlight></codeline>
<codeline><highlight class="normal">bounds<sp/>=<sp/>renderer-&gt;GetOpaqueBoundsInPixels(1.0f,<sp/>96.0f,<sp/>150.0f);</highlight></codeline>
<codeline><highlight class="normal"></highlight></codeline>
<codeline><highlight class="normal">ASSERT_EQ(159,<sp/>bounds.get_Width());</highlight></codeline>
<codeline><highlight class="normal">ASSERT_EQ(30,<sp/>bounds.get_Height());</highlight></codeline>
</programlisting> </para>
